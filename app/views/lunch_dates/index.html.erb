<h1>Listing lunch_dates</h1>

<table>
  <tr>
    <th>Lunch date</th>
    <th>Cutoff date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @lunch_dates.each do |lunch_date| %>
  <tr>
    <td><%= lunch_date.lunch_date %></td>
    <td><%= lunch_date.cutoff_date %></td>
    <td><%= link_to 'Show', lunch_date %></td>
    <td><%= link_to 'Edit', edit_lunch_date_path(lunch_date) %></td>
    <td><%= link_to 'Destroy', lunch_date, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

